{
  "name": "instagram-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "tw:dev": "postcss ./src/tailwind.src.css -o ./src/tailwind.css",
    "start:tw": "postcss ./src/tailwind.src.css -o ./src/tailwind.css -w",
    "start": "npm run tw:dev && react-scripts start",
    "build:tw": "postcss ./src/tailwind.src.css -o ./src/tailwind.css --env production",
    "build": "npm run build:tw && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "postcss-cli": "^6.1.2",
    "styled-components": "^4.2.0",
    "tailwindcss": "^1.0.1"
  }
}
